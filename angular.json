{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "newProjectRoot": "projects",
  "projects": {
    "unofficial-ngx-anilist-client": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "production": {
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                },
                {
                  "maximumWarning": "6kb",
                  "type": "anyComponentStyle"
                }
              ],
              "buildOptimizer": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "vendorChunk": false
            }
          },
          "defaultConfiguration": "",
          "options": {
            "assets": ["src/favicon.ico", "src/assets"],
            "buildOptimizer": false,
            "extractLicenses": false,
            "index": "src/index.html",
            "main": "src/main.ts",
            "namedChunks": true,
            "optimization": false,
            "outputPath": "dist/unofficial-ngx-anilist-client",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "sourceMap": true,
            "stylePreprocessorOptions": {
              "includePaths": ["src/app/styles"]
            },
            "styles": [
              "node_modules/@fortawesome/fontawesome-free/css/fontawesome.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/brands.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/solid.min.css",
              "src/app/styles/vanilla-extended.scss",
              "src/app/styles/mat-theme.scss",
              "src/app/styles/mat-overrides.scss",
              "src/app/styles/cards.scss",
              "src/app/styles/color-utils.scss",
              "src/app/styles/devices.scss",
              "src/app/styles/font-sizes.scss",
              "src/app/styles/spacing.scss",
              "src/app/styles/media-table.scss",
              "src/app/styles/data-display.scss",
              "src/app/styles/utility.scss",
              "src/styles.scss"
            ],
            "tsConfig": "src/tsconfig.app.json",
            "vendorChunk": true
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "unofficial-ngx-anilist-client:build:production"
            }
          },
          "options": {
            "buildTarget": "unofficial-ngx-anilist-client:build"
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "src"
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    },
    "@schematics/angular:service": {
      "type": "service"
    }
  },
  "version": 1
}
