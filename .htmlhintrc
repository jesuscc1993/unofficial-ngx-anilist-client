{
  "alt-require": true,
  "attr-lowercase": false, // incompatible with angular, as it does not follow this rule
  "attr-no-duplication": true,
  "attr-unsafe-chars": true,
  "attr-value-double-quotes": true,
  "attr-value-not-empty": false, // incompatible with angular, as it uses empty attributes
  "doctype-first": false, // incompatible with angular, as we do not include it in component templates
  "empty-tag-not-self-closed": false, // incompatible with prettier
  "head-script-disabled": true,
  "href-abs-or-rel": true,
  "id-class-ad-disabled": true,
  "id-class-value": true,
  "id-unique": true,
  "inline-script-disabled": true,
  "inline-style-disabled": true,
  "space-tab-mixed-disabled": true,
  "spec-char-escape": true,
  "src-not-empty": true,
  "style-disabled": true,
  "tag-pair": true,
  "tagname-lowercase": true,
  "title-require": true
}
